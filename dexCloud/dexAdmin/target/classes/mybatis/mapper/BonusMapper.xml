<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dex.dao.BonusDao">

    <select id="adminGetAllBonus" resultType="domain.bonusPojo">
        select * from bonus ORDER BY id DESC;
    </select>

    <select id="adminGetAllBonusOrder" resultType="domain.bonusOrderPojo">
        select * from bonusorder ORDER BY id DESC;
    </select>

</mapper>
